{
  "name": "Go Install Packages",
  "id": "go-packages-install",
  "version": "0.0.2",
  "description": "Installs go packages",
  "options": {
    "PACKAGES": {
      "type": "string",
      "description": "package-names, example: golang.org/x/tools/gopls@latest,github.com/cosmtrek/air@latest",
      "default": ""
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils",
    "ghcr.io/devcontainers/features/go"
  ]
}